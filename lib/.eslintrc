{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/prefer-es6-class": [0],
    "react/jsx-no-bind": [0],
    "no-param-reassign": [0],
    "arrow-body-style":[0],
    "id-length": [0],
    "indent": [2, 2, {"SwitchCase": 1}]
  },
  "env": {
    "node": true,
    "mocha": true
  }
}
